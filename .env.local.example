# Server-side only. DO NOT expose this in the browser.
OPENAI_API_KEY=sk-your-key-here

# Realtime voice session config
OPENAI_REALTIME_MODEL=gpt-realtime
OPENAI_VOICE=marin

# ASR / transcription model used by the Realtime session for input transcription (optional)
OPENAI_ASR_MODEL=gpt-4o-mini-transcribe

# Text model used by the extraction route (structured output)
OPENAI_EXTRACT_MODEL=gpt-4o-mini
